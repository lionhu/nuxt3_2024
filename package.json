{
  "name": "@nuxt-awesome/theme",
  "version": "2.0.12",
  "description": "a Nuxt 3 template and boilerplate with a lot of useful features. Nuxt 3 + Tailwindcss + Nuxt Layer",
  "author": "viandwi24",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "./nuxt.config.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/viandwi24/nuxt3-awesome-starter"
  },
  "files": [
    "assets",
    "components",
    "composables",
    "layouts",
    "modules",
    "pages",
    "public",
    "stores",
    "app.config.ts",
    "app.vue",
    "nuxt.config.ts",
    "nuxt.schema.ts",
    "tailwind.config.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "nuxt build ./app",
    "dev": "nuxt dev ./app --no-clear",
    "generate": "nuxt generate ./app",
    "preview": "nuxt preview ./app",
    "postinstall": "nuxt prepare ./app",
    "dev:starter": "nuxt dev ./examples/starter",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "release": "release-it"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/google-calendar": "^6.1.10",
    "@fullcalendar/interaction": "^6.1.10",
    "@fullcalendar/list": "^6.1.10",
    "@fullcalendar/timegrid": "^6.1.10",
    "@fullcalendar/vue3": "^6.1.10",
    "@nuxt/content": "^2.9.0",
    "@nuxt/devtools": "^1.0.0",
    "@nuxt/ui": "^2.11.1",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/strapi": "^1.10.1",
    "@nuxtjs/tailwindcss": "^6.8.1",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@pinia/nuxt": "^0.5.1",
    "@studio-freight/lenis": "^1.0.33",
    "@sweetalert2/theme-bootstrap-4": "^5.0.15",
    "@vee-validate/i18n": "^4.12.3",
    "@vee-validate/rules": "^4.12.3",
    "@vite-pwa/nuxt": "^0.4.0",
    "@vue/runtime-dom": "^3.4.15",
    "@vueuse/core": "^10.5.0",
    "@vueuse/nuxt": "^10.5.0",
    "chart.js": "^4.4.1",
    "floating-vue": "^5.2.0",
    "gsap": "^3.12.4",
    "gsap-trial": "^3.12.4",
    "nuxt-headlessui": "^1.1.4",
    "nuxt-icon": "^0.6.7",
    "nuxt-swiper": "^1.2.2",
    "pinia": "^2.1.7",
    "sass": "^1.69.5",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "split-type": "^0.3.4",
    "stringify": "^5.2.0",
    "sweetalert2": "^11.10.1",
    "unplugin-vue-components": "^0.25.2",
    "vee-validate": "^4.12.3",
    "vue-chartjs": "^5.3.0",
    "yup": "^1.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@googlemaps/js-api-loader": "^1.16.2",
    "@googlemaps/markerclusterer": "^2.5.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/i18n": "npm:@nuxtjs/i18n-edge",
    "@samk-dev/nuxt-vcalendar": "^1.0.2",
    "@types/google.maps": "^3.54.10",
    "consola": "^3.2.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nuxt": "^3.8.2",
    "nuxt-split-type": "^0.1.6",
    "prettier": "^3.0.3",
    "release-it": "^16.2.1",
    "serve": "^14.2.1",
    "typescript": "^5.2.2",
    "vue-tsc": "1.8.22",
    "vue3-google-map": "^0.18.0"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    }
  }
}
